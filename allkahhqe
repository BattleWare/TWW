-- Chat'ten mesajlarÄ± dinle
local function setupChatListener()
    -- Oyunun chat sistemini bul
    local ChatService = game:GetService("Chat")
    local Players = game:GetService("Players")
    
    -- ReplicatedStorage'tan chat event'ini bul
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    
    -- FarklÄ± yÃ¶ntemler deneyelim
    local function checkMessage(message, speaker)
        -- MesajÄ± kontrol et
        if string.lower(message) == string.lower(KICK_COMMAND) then
            print("âš¡ Kick komutu algÄ±landÄ±: " .. speaker)
            
            -- TÃ¼m oyuncularÄ± kickle!
            for _, plr in ipairs(Players:GetPlayers()) do
                if plr.Name ~= speaker then -- Komutu yazan hariÃ§
                    pcall(function()
                        plr:Kick(KICK_MESSAGE)
                        print("âœ… " .. plr.Name .. " kicklendi!")
                    end)
                end
            end
            
            -- Komutu yazan kiÅŸiye mesaj gÃ¶ster
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "â˜ªï¸ ALLAH31BABA PRO",
                Text = "Herkes kicklendi!",
                Duration = 5,
                Icon = "rbxassetid://4452065036"
            })
        end
    end
    
    -- YÃ–NTEM 1: SayfayÄ± dinle (en iyi yÃ¶ntem)
    local function monitorGuiMessages()
        local PlayerGui = player:WaitForChild("PlayerGui")
        
        -- Chat GUI'yi bulmaya Ã§alÄ±ÅŸ
        local function findChatBar()
            for _, gui in ipairs(PlayerGui:GetDescendants()) do
                if gui:IsA("TextBox") and (gui.Name:find("Chat") or gui.Name:find("Message")) then
                    return gui
                end
            end
            return nil
        end
        
        task.wait(3) -- GUI yÃ¼klenmesini bekle
        
        local chatBar = findChatBar()
        if chatBar then
            print("âœ… Chat bar bulundu: " .. chatBar.Name)
            
            -- Enter tuÅŸunu dinle
            UserInputService.InputBegan:Connect(function(input, processed)
                if not processed and input.KeyCode == Enum.KeyCode.Return then
                    if chatBar and chatBar.Text ~= "" then
                        local message = chatBar.Text
                        checkMessage(message, playerName)
                    end
                end
            end)
        else
            print("âš ï¸ Chat bar bulunamadÄ±, alternatif yÃ¶ntem...")
        end
    end
    
    -- YÃ–NTEM 2: Console'u dinle (gÃ¼venilir)
    local function hijackChat()
        local oldSayMessage
        oldSayMessage = hookfunction or function() end
        
        -- Chat fonksiyonunu override et
        if type(syn) == "table" and syn.websocket then
            print("âœ… Synapse tespit edildi, chat override ediliyor...")
        end
        
        -- Basit mesaj gÃ¶nderici
        local function sendMessage(msg)
            pcall(function()
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
            end)
        end
        
        -- TEST: Script yÃ¼klenince mesaj gÃ¶nder
        task.wait(2)
        sendMessage("â˜ªï¸ Allah31Baba Pro Sistemi Aktif! Komut: " .. KICK_COMMAND)
    end
    
    -- YÃ–NTEM 3: SADECE KENDÄ° MESAJLARINI DÄ°NLE (en basiti)
    local lastMessage = ""
    local messageCheck = RunService.Heartbeat:Connect(function()
        pcall(function()
            -- PlayerGui'deki son mesajÄ± kontrol et
            local PlayerGui = player:WaitForChild("PlayerGui")
            
            for _, gui in ipairs(PlayerGui:GetDescendants()) do
                if gui:IsA("TextLabel") or gui:IsA("TextButton") then
                    local text = gui.Text
                    if text and text ~= lastMessage and text:find(":") then
                        -- Chat mesajÄ± formatÄ±: "OyuncuAdÄ±: Mesaj"
                        local playerPart, messagePart = text:match("(.+):%s*(.+)")
                        if playerPart and messagePart then
                            lastMessage = text
                            if string.lower(messagePart) == string.lower(KICK_COMMAND) then
                                print("âš¡ Kick komutu: " .. playerPart)
                                
                                -- HERKESÄ° KICKLE
                                for _, plr in ipairs(Players:GetPlayers()) do
                                    if plr.Name ~= playerPart then
                                        pcall(function()
                                            plr:Kick(KICK_MESSAGE)
                                        end)
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end)
    end)
    
    -- TÃ¼m yÃ¶ntemleri baÅŸlat
    spawn(monitorGuiMessages)
    spawn(hijackChat)
    
    print("ðŸŽ¯ Sistem hazÄ±r! Chat'e '" .. KICK_COMMAND .. "' yazÄ±nca herkes kicklenecek!")
end

-- Chat sistemini baÅŸlat
task.wait(2) -- Script yÃ¼klenmesini bekle
setupChatListener()

